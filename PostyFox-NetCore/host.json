  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": { "default": "Debug" },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request",
        "minSamplingPercentage": 100
      },
      "enableLiveMetricsFilters": true
    }
  }
}